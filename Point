package code0204;

public class Point {
int x=0;
int y=0;
public void move(int dx,int dy)
{
	x=x+dx;
	y=y+dy;
}
public void alert() {
	System.out.println("x="+x+"y="+y);
}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
Point p=new Point();
p.move(1, 2);
p.alert();
	}

}
